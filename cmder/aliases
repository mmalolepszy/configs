e.=explorer .
ls=ls --show-control-chars -F --color $*
pwd=cd
clear=cls
history=cat %CMDER_ROOT%\config\.history
unalias=alias /d $1
ll=ls -lh --show-control-chars -F --color $*  
la=ls -a --show-control-chars -F --color $*  
home=cd /d c:\Users\mmalolepszy\  
projects=cd /d c:\Projects\  
dn=dotnet $*  
gs=git status -sb $*  
gla=git log --oneline --all --graph --decorate $*  
gl=git log --oneline --all --graph --decorate -n15 $*  
